[MASTER]
ignore=venv,migrations
load-plugins=pylint_django

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    too-many-locals,
    too-many-branches

[FORMAT]
max-line-length=100

[DESIGN]
max-parents=10
max-attributes=10
max-locals=25
